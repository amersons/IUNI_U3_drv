//Gionee liujiang 2013-07-31 add for button led start
&pm8941_lsid0 {
   	qcom,leds@a700 {
		status = "okay";
		qcom,led_mpp_8 {
			label = "mpp";
			linux,name = "button-backlight";
			linux,default-trigger = "none";
			qcom,default-state = "off";
			qcom,max-current = <40>;
			qcom,current-setting = <5>;
			qcom,id = <6>;
			qcom,mode = "manual";
			qcom,source-sel = <1>;
			qcom,mode-ctrl = <0x60>;
		};
	};
};
